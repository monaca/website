<script>
  window.addEventListener("load", function() {
    $('body').click(function(e){
      $target = $(e.target);

      //support-type-balloon
      if($target.is('#show_support_type')){
        $('#support-type-balloon').toggle();
        return false;
      } else if($(e.target).parents('#support-type-balloon').length == 0){
        $('#support-type-balloon').hide();
      }

      //support-ticket-balloon
      if($target.is('#show_support_ticket')){
        $('#support-ticket-balloon').toggle();
        return false;
      } else if($(e.target).parents('#support-type-balloon').length == 0){
        $('#support-ticket-balloon').hide();
      }
    }
    );
  });
</script>